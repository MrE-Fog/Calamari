<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <AssemblyName>Sashimi.GoogleCloud.Scripting</AssemblyName>
        <PackageProjectUrl>https://github.com/OctopusDeploy/Sashimi.GCP</PackageProjectUrl>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <NoDefaultExcludes>true</NoDefaultExcludes>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\..\artifacts\Calamari.GoogleCloudScripting.zip">
            <Visible>false</Visible>
            <LinkBase>tools</LinkBase>
            <Pack>true</Pack>
            <PackagePath>tools/</PackagePath>
        </None>
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="Sashimi.Server.Contracts" Version="13.3.0" />
    </ItemGroup>

</Project>
