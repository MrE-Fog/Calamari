<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Sashimi.AzureWebAppZip</AssemblyName>
    <RootNamespace>Sashimi.AzureWebAppZip</RootNamespace>
    <TargetFramework>netstandard2.1</TargetFramework>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <Nullable>enable</Nullable>
    <OutputPath>bin\</OutputPath>
    <PackageProjectUrl>https://github.com/octopusDeploy/Sashimi.AzureWebAppZip</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>NU5119</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Octopus.Configuration" Version="2.1.0" />
    <PackageReference Include="Sashimi.Azure.Accounts" Version="8.3.1" />
    <PackageReference Include="Sashimi.Azure.Common" Version="8.3.1" />
    <PackageReference Include="Sashimi.AzureScripting" Version="8.3.1" />
  </ItemGroup>

</Project>
